# Environment Variables for aqua-blue Price Prediction
# Copy this file to .env and fill in your actual values

# Polygon.io API Key
# Get your free API key from: https://polygon.io/
POLYGON_API_KEY=polygon_api_key

# For Flat Files
POLYGON_S3_ACCESS_KEY=your_s3_access_key
POLYGON_S3_SECRET_KEY=your_s3_secret_key


# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: Default prediction settings
DEFAULT_SYMBOL=AAPL
DEFAULT_TRAINING_WINDOW=500
DEFAULT_PREDICTION_HORIZON=10 